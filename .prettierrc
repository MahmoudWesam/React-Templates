{
  "printWidth": 120,
  "arrowParens": "always",
  "semi": false,
  "tabWidth": 2,
  "endOfLine": "auto",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^react(.*)",
    "^types$",
    "^@/env(.*)$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "@/(.*)",
    "^[./].*(?<!\\.(c|sc)ss)$",
    "^./*.*.css"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "COMMENT:": "// A boolean value to enable or disable sorting the namespace specifiers to the top of the import group.",
  "importOrderGroupNamespaceSpecifiers": true
}
